{
  "name": "thefoovault/sf-skeleton",
  "autoload": {
    "psr-4": {
      "Shared\\": "src/Shared",
      "SymfonyClient\\": "apps/SymfonyClient/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "extra": {
    "bin-dir": "apps/SymfonyClient/bin",
    "config-dir": "apps/SymfonyClient/config",
    "src-dir": "apps/SymfonyClient/src",
    "var-dir": "apps/SymfonyClient/var",
    "public-dir": "apps/SymfonyClient/public"
  },
  "require": {
    "symfony/flex": "^1.13",
    "symfony/framework-bundle": "^5.4",
    "symfony/dotenv": "^5.4",
    "symfony/yaml": "^5.4",
    "symfony/console": "^5.4",
    "symfony/runtime": "^5.4",
    "symfony/messenger": "^5.4",
    "ramsey/uuid": "^4.2",
    "symfony/serializer": "^5.4",
    "symfony/property-access": "^5.4",
    "runtime/swoole": "^0.3.1",
    "openswoole/core": "^22.1",
    "ext-openswoole": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "dg/bypass-finals": "^1.3",
    "openswoole/ide-helper": "^22.0"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
